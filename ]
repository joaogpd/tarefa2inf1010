#include <stdio.h>
#include <stdlib.h>
#include "queue.h"

typedef struct bintree BinTree;

typedef struct no No;
struct no {
    BinTree* data;
    No* next;
};

typedef struct fila Fila;
struct fila {
    No* ini;
    No* fim;
};

Fila* fila_cria(void) {
    Fila* novo = (Fila*)malloc(sizeof(Fila));
    novo->ini = novo->fim = NULL;
    return novo;
}

Fila* fila_insere(Fila* f, BinTree* b) {
    No* novo_no = (No*)malloc(sizeof(No));
    novo_no->data = b;
    novo_no->next = NULL;
    if (f->ini == NULL) {
	f->ini = f->fim = novo_no;
	return novo;
    }
    f->fim->next = novo_no;
    f->fim = novo_no;
    return f;
}

Fila* fila_retira(Fila* f) {
    if (f->ini == NULL) 
        return f;
    No* temp = f->ini;
    f->ini = f->ini->next;
    if (f->ini == NULL) 
	f->fim = NULL;
    free(temp);
    return f;
}

Fila* fila_libera(Fila* f) {
    No* r = NULL;
    No* p = f->ini;
    while (p != NULL) {
        r = p;
	p = p->next;
	free(r);
    }
    return NULL;
}
